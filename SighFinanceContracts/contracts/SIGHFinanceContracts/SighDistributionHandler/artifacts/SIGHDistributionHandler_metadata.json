		"devdoc": {
			"methods": {
				"Instrument_SIGHed(address)": {
					"details": "Instrument to be convered under the SIGH DIstribution Mechanism - Decided by the SIGH Finance Manager who  can call this function through the Sigh Finance Configurator",
					"params": {
						"instrument_": "the instrument object*"
					}
				},
				"Instrument_UNSIGHed(address)": {
					"details": "Instrument to be removed from SIGH DIstribution Mechanism - Decided by the SIGH Finance Manager who  can call this function through the Sigh Finance Configurator",
					"params": {
						"instrument_": "the instrument object*"
					}
				},
				"SpeedUpperCheckSwitch(bool,uint256)": {
					"params": {
						"isActivated": "Boolean deciding if the upper check on SIGH speed (loss driven) activated or not",
						"profitPercentage": "The additional profit based SIGH distribution over and above the covered losses"
					}
				},
				"addInstrument(address,address,uint256)": {
					"details": "adds an instrument - Called by LendingPool Core when an instrument is added to the Lending Protocol",
					"params": {
						"_decimals": "the number of decimals of the underlying asset*",
						"_iTokenAddress": "the address of the overlying iToken contract",
						"_instrument": "the instrument object"
					}
				},
				"transferSighTotheUser(address,address,address,uint256)": {
					"details": "Note: If there is not enough SIGH, we do not perform the transfer call.",
					"params": {
						"instrument": "The instrument for which the SIGH has been accured",
						"sigh_Amount": "The amount of SIGH to (possibly) transfer",
						"user": "The address of the user to transfer SIGH to"
					},
					"return": "The amount of SIGH which was NOT transferred to the user"
				},
				"updateDeltaBlocksForSpeedRefresh(uint256)": {
					"params": {
						"deltaBlocksLimit": "The new Minimum blocks limit"
					}
				},
				"updateSIGHBorrowIndex(address)": {
					"params": {
						"currentInstrument": "The market whose borrow index to update"
					}
				},
				"updateSIGHSpeed(uint256)": {
					"params": {
						"SIGHSpeed_": "The amount of SIGH wei per block to distribute"
					}
				},
				"updateSIGHSupplyIndex(address)": {
					"params": {
						"currentInstrument": "The Instrument whose supply index to update"
					}
				},
				"updateStakingSpeedForAnInstrument(address,uint256)": {
					"params": {
						"instrument_": "The instrument",
						"newStakingSpeed": "The additional SIGH staking speed assigned to the Instrument"
					}
				}
			}
		},
		"userdoc": {
			"methods": {
				"SpeedUpperCheckSwitch(bool,uint256)": {
					"notice": "Activates / Deactivates the upper check and sets the profit percentage for an Instrument - Decided by the SIGH Finance Manager who  can call this function through the Sigh Finance Configurator"
				},
				"refreshSIGHSpeeds()": {
					"notice": "Recalculate and update SIGH speeds for all Supported SIGH markets"
				},
				"transferSighTotheUser(address,address,address,uint256)": {
					"notice": "Transfer SIGH to the user. Called by the corresponding IToken Contract of the instrument"
				},
				"updateDeltaBlocksForSpeedRefresh(uint256)": {
					"notice": "Updates the minimum blocks to be mined before speed can be refreshed again  - Decided by the SIGH Finance Manager who  can call this function through the Sigh Finance Configurator"
				},
				"updateSIGHBorrowIndex(address)": {
					"notice": "Accrue SIGH to the market by updating the borrow index"
				},
				"updateSIGHSpeed(uint256)": {
					"notice": "Sets the amount of Global SIGH distributed per block - - Decided by the SIGH Finance Manager who  can call this function through the Sigh Finance Configurator"
				},
				"updateSIGHSupplyIndex(address)": {
					"notice": "Accrue SIGH to the Instrument by updating the supply index"
				},
				"updateStakingSpeedForAnInstrument(address,uint256)": {
					"notice": "Sets the staking speed for an Instrument - Decided by the SIGH Finance Manager who  can call this function through the Sigh Finance Configurator"
				}
			}
		}
	},
